<% if user_signed_in? %>
  <p><%= current_user.phone %></p>
  <p><%= current_user %></p>
  <p><%= link_to "Log out", destroy_user_session_path, method: :delete %></p>
<% else %>
  <p><%= link_to "Login", new_user_session_path %></p>
  <p><%= link_to "Signup", new_user_registration_path %></p>
<% end %>